<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
        <attr name="pstsSelectedTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="AppGuideButton">
        <attr name="guideBtnColor" format="integer|reference|color" />
        <attr name="guideBtnText" format="string" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="IndicatorView">
        <!-- 当前索引图标 -->
        <attr name="active" format="reference" />
        <!-- 非当前索引图标 -->
        <attr name="unActive" format="reference" />
        <!-- 指示标志间距 -->
        <attr name="space" format="dimension" />
        <attr name="total" format="integer" />
    </declare-styleable>

    <!--自定义title-->
    <declare-styleable name="MyTitle">
        <!--坐标back图标-->
        <attr name="setLeftActionIcon" format="reference" />
        <!--中间title-->
        <attr name="setTitleTxt" format="string" />
        <!--右边图标一（search）-->
        <attr name="setRightIconL" format="reference" />
        <!--右边图标二（action）-->
        <attr name="setRightActIconR" format="reference" />
        <!--title字体颜色-->
        <attr name="setTitleTxtColor" format="color|integer|reference" />
    </declare-styleable>

    <!--自定义checkbox-->
    <declare-styleable name="MyCheckView">
        <!--背景框-->
        <attr name="backgroundIcon" format="reference" />
        <!--check图标-->
        <attr name="checkIcon" format="reference" />
    </declare-styleable>


</resources>